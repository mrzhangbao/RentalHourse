<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.jiaxin.entity.User" table="tb_user" catalog="db_rentalhourse">
		<id name="id" type="java.lang.Integer">
			<column name="id" length="50" />
			<generator class="increment" />
		</id>

		<property name="phoneNum" type="java.lang.String">
			<column name="phoneNum" length="20" />
		</property>
		<property name="email" type="java.lang.String">
			<column name="email" length="120" />
		</property>
		<property name="password" type="java.lang.String">
			<column name="password" length="120" />
		</property>
		<property name="nickName" type="java.lang.String">
			<column name="nickname" length="50" />
		</property>
		<property name="realName" type="java.lang.String">
			<column name="realname" length="50" />
		</property>
		<property name="company" type="java.lang.String">
			<column name="company" length="50" />
		</property>
		<property name="role" type="java.lang.String">
			<column name="role" length="50" />
		</property>
		<property name="avatar" type="java.lang.String">
			<column name="avatar" length="150" />
		</property>
		<property name="lastSign" type="java.sql.Timestamp">
			<column name="lastSign" length="50" />
		</property>
		<property name="creditNum" type="java.lang.String">
			<column name="creditNum" length="150" />
		</property>
		<property name="collection" type="java.lang.String">
			<column name="collection" length="50" />
		</property>
		<property name="creditScore" type="java.lang.String">
			<column name="creditScore" length="50" />
		</property>
		<property name="createTime" type="java.sql.Timestamp">
			<column name="createTime" length="50" />
		</property>
	</class>
</hibernate-mapping>